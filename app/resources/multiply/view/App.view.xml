<mvc:View controllerName="sap.sample.multiply.controller.App" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" class="sapUiSizeCompact">
	<Panel id="Panel1" headerText="Service Test - Multiplication" expandable="true" expanded="true">
		<List id="List1" width="400px">
			<InputListItem id="Val1Item" label="Value #1" >
				<Input id="val1" value="{/val1}" liveChange="onLiveChange"/>
			</InputListItem>
			<InputListItem id="lblValue2" label="Value #2" >
				<Input id="val2" value="{/val2}" liveChange="onLiveChange"/>
			</InputListItem>
			<InputListItem id="lblResults" label="=">
				<Text id="result" text="{/result}"/>
			</InputListItem>
		</List>
	</Panel>
</mvc:View>